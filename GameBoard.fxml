                <Font name="FreeSans Bold" size="13.0" />
            </font>
            <GridPane.margin>
                <Insets left="12.0" top="35.0" />
            </GridPane.margin>
        </Label>
        <Label fx:id="blackPlayerScore" alignment="CENTER" prefHeight="30.0" prefWidth="150.0" textAlignment="LEFT" textFill="#4b86eb" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.valignment="TOP">
