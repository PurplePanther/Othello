            <GridPane.margin>
                <Insets left="0.0" />
            </GridPane.margin></Label>
        <Label fx:id="currentPlayer" alignment="CENTER" prefHeight="30.0" prefWidth="150.0" textAlignment="LEFT" textFill="#4b86eb" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.valignment="TOP">
