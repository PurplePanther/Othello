      <Label fx:id="winnerPlayer" alignment="CENTER" contentDisplay="CENTER" prefHeight="58.0" prefWidth="200.0" textFill="#4b86eb" GridPane.columnIndex="1">
         <font>
            <Font name="FreeSans Bold" size="13.0" />
