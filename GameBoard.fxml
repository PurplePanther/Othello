                <Font name="FreeSans Bold" size="13.0" />
            </font>
            <GridPane.margin>
                <Insets right="-20.0" />
            </GridPane.margin>
        </Label>
        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="30.0" prefWidth="150.0" text="BLACK PLAYER SCORE:" textAlignment="LEFT" textFill="#4b86eb" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.valignment="TOP">
