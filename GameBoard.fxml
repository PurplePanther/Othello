      <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0" />
      <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0" />
    </rowConstraints>

    <children>
        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="30.0" prefWidth="150.0" text="CURRENT PLAYER:" textAlignment="LEFT" textFill="#4b86eb" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.valignment="TOP">
